include ../rooms-list/rooms-list.pug
include ../rooms-filter/rooms-filter.pug

mixin catalog() 
    div.catalog
        div.catalog__container.container
            div.catalog__title catalog-title 
            div.catalog__rooms-select rooms-select 
        div.catalog__container.container 
            div.catalog__rooms-filter 
                +rooms-filter()
            div.catalog__room-list 
                +rooms-list() 
